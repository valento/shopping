    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "generate-password": "^1.4.1",
    "jsonwebtoken": "^8.4.0",
    "sqlite3": "^4.0.4"
  }
